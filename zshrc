# Basics
export ZSH=/home/soomink/.oh-my-zsh
ZSH_THEME="af-magic"
plugins=(git)
source $ZSH/oh-my-zsh.sh

# Set from Soomin Kim

# Update
export UPDATE_ZSH_DAYS=1

# Colors
export TERM=xterm-256color
LS_COLORS='di=01;94:'
export LS_COLORS

# Alias
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gp='git pull'
alias gd='git diff'
